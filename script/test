#!/bin/sh

cljr help
cljr list
cljr search incanter
cljr describe incanter
cljr versions incanter
cljr install incanter
cljr list
cljr add-classpath /Users/dliebke/Desktop/dev/incanter
cljr classpath
cljr list-jars
cljr run ~/Desktop/dev/incanter/examples/run_prob_plots.clj
cljr repl
cljr swingrepl
cljr swank
cljr remove-classpath /Users/dliebke/Desktop/dev/incanter
cljr remove incanter
cljr list
cljr clean
cljr reload
